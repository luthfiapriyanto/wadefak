<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_marginRight="8dp"
    android:layout_marginLeft="8dp"
    android:layout_marginBottom="7dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/imageView6"
        android:background="@drawable/icon_arr_li"
        android:layout_width="16dp"
        android:layout_height="16dp" />
    <TextView
        android:textColor="@color/title"
        android:textSize="15sp"
        android:text="mencoba"
        android:id="@+id/list_item_available_outlet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView6"
        android:layout_toEndOf="@+id/imageView6"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:background="@android:drawable/ic_menu_call"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignBottom="@+id/list_item_available_outlet"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>